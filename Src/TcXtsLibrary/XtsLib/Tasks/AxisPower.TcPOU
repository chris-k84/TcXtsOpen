  <POU Name="AxisPower" Id="{cf8d7780-536f-48f2-8eb4-f2d08824929f}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK AxisPower EXTENDS TaskBase
VAR
	mcPower : Tc2_MC2.MC_Power;
	AxisReference : REFERENCE TO Tc2_MC2.AXIS_REF;
	state : INT;
	EnableAxis : BOOL;
	_busy : BOOL;
	_error : BOOL;
	_done : BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Method Name="Abort" Id="{6b214ba9-44b4-4668-9ec2-8eddbec189ec}">
      <Declaration><![CDATA[METHOD Abort : BOOL]]></Declaration>
      <Implementation>
        <ST><![CDATA[mcPower.Enable := mcPower.Enable_Negative := mcPower.Enable_Positive := FALSE;
_busy := FALSE;
EnableAxis := FALSE;
Abort := TRUE;
State := 0;]]></ST>
      </Implementation>
    </Method>
    <Property Name="Busy" Id="{ef0f4437-de71-4221-a0af-37624a426c16}">
      <Declaration><![CDATA[PROPERTY Busy : BOOL
]]></Declaration>
      <Get Name="Get" Id="{811a31bb-e198-434f-898d-917e03bf6183}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[Busy := _busy;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Method Name="Cycle" Id="{49735321-892e-4ec1-aa25-952ce1fe7601}">
      <Declaration><![CDATA[METHOD Cycle : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE State OF
	0:
		IF EnableAxis THEN
			_done := FALSE;
			_error := FALSE;
			_busy := TRUE;
			mcPower.Enable := mcPower.Enable_Negative := mcPower.Enable_Positive := TRUE;
			mcPower.Override := 100;
			State := 10;
		END_IF
	
	10:
		IF mcPower.Status THEN
			_busy := FALSE;
			_done := TRUE;
			state := 20;
		END_IF
	
	20:
		IF NOT EnableAxis THEN
			_done := FALSE;
			_busy := TRUE;
			mcPower.Enable := mcPower.Enable_Negative := mcPower.Enable_Positive := FALSE;
			State := 30;
		END_IF
	30:
		IF NOT mcPower.Status THEN
			_busy := FALSE;
			_done := TRUE;
			state := 0;
		END_IF
END_CASE

mcPower(Axis := AxisReference);]]></ST>
      </Implementation>
    </Method>
    <Method Name="Disable" Id="{71e12fcb-ea2b-4a80-97e6-c20b6299e2da}">
      <Declaration><![CDATA[METHOD Disable : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT EnableAxis THEN
	RETURN;
END_IF
Disable := TRUE;
EnableAxis := FALSE;
]]></ST>
      </Implementation>
    </Method>
    <Property Name="Done" Id="{3edec79a-21aa-4354-b705-f4ba8d01278c}">
