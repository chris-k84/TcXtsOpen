END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[Done := _done;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Method Name="Enable" Id="{a6939086-c52d-4f49-9683-237a63574b37}">
      <Declaration><![CDATA[METHOD Enable : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF EnableAxis THEN
	RETURN;
END_IF
Enable := EnableAxis := true;]]></ST>
      </Implementation>
    </Method>
    <Property Name="Error" Id="{8a127411-a21f-4341-9085-d8abfe79a613}">
