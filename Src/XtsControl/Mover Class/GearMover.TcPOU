	XtsTrack : IXtsTrack;
	StateMachine : IPmlStateMachine;
END_VAR
VAR_OUTPUT
END_VAR
VAR
	moverSequence : GearMoverSequence;
	bInit : BOOL;
	CurrentLimit AT%Q* : INT;
	Mode AT%Q* : UINT;
END_VAR
VAR_IN_OUT
	MoverArray : ARRAY[*] OF IMover;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF NOT bInit THEN
	bInit := TRUE;
	_trackLength := XtsTrack.TrackLength;
	IF (THIS^._axis.NcToPlc.AxisId MOD 2)<>1 THEN
		Mode.8 := TRUE;
		CurrentLimit := 3000;
	END_IF
END_IF

SUPER^();

moverSequence(ThisMover := THIS^,
				XtsTrack := XtsTrack,
				StateMachine := StateMachine,
				MoverArray := MoverArray);
				
]]></ST>
