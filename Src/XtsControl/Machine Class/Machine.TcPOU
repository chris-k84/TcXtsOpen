    <Method Name="SetPackML" Id="{53c56351-61ed-4063-8940-94e0a76b1fed}">
      <Declaration><![CDATA[{attribute 'TcRpcEnable':='1'}
METHOD SetPackML : BOOL
VAR_INPUT
	state : PackMLCommand;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE state OF
	PackMLCommand.Abort:
		stateMachine.PMLCommand(Command := E_PMLCommand.ePMLCommand_Abort);
	
	PackMLCommand.Reset: 
		stateMachine.PMLCommand(Command := E_PMLCommand.ePMLCommand_Reset);
	
	PackMLCommand.Clear:
		stateMachine.PMLCommand(Command := E_PMLCommand.ePMLCommand_Clear);
	
	PackMLCommand.Start:
		stateMachine.PMLCommand(Command := E_PMLCommand.ePMLCommand_Start);
END_CASE]]></ST>
      </Implementation>
    </Method>
