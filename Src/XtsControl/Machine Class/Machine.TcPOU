  <POU Name="Machine" Id="{e8d4edde-6e68-4e80-9f5b-2e9346044142}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK Machine IMPLEMENTS IMachine
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	Xts : XtsTrack := (TrackLength := SystemParameters.TrackLength, HomePosition := SystemParameters.TrackHomePosition, Tracks := SystemParameters.Tracks);
	stateMachine : PMLStateMachine;
	Movers : ARRAY[1..SystemParameters.NumberOfMovers] OF FB_Mover;
	iMovers : ARRAY[1..SystemParameters.NumberOfMovers] OF IMover;// := [indexMovers[1], indexMovers[2]];
	MoverSequences : ARRAY[1..SystemParameters.NumberOfMovers] OF IndexMoverSequence;
	StationArray : ARRAY[1..SystemParameters.NumberOfStations] OF StationSample;
	iStationArray : ARRAY[1..SystemParameters.NumberOfStations] OF IProcess;
	i: INT;
	bInit: BOOL;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF NOT bInit THEN
	FOR i := 1 TO Xts_Library.SystemParameters.NumberOfMovers DO
		iMovers[i] := Movers[i];
		Movers[i].TrackLength := Xts.TrackLength;
	END_FOR
	IF InitialiseStations() THEN
		bInit := TRUE;
	END_IF
END_IF

stateMachine();
Xts(MoverArray := iMovers, StateMachine := stateMachine);
FOR i := 1 TO Xts_Library.SystemParameters.NumberOfMovers DO
	Movers[(Xts_Library.SystemParameters.NumberOfMovers+1)-i]();
	MoverSequences[(Xts_Library.SystemParameters.NumberOfMovers+1)-i](ThisMover := Movers[(Xts_Library.SystemParameters.NumberOfMovers+1)-i], 
																		XtsTrack := Xts, 
																		StateMachine := stateMachine, 
																		StationArray := iStationArray);																
END_FOR
FOR i := 1 TO Xts_Library.SystemParameters.NumberOfStations DO
	StationArray[i](StateMachine := stateMachine);
END_FOR
]]></ST>
    </Implementation>
    <Method Name="InitialiseStations" Id="{484207a2-fa7d-4c77-9d9b-69f5cd52cf18}">
      <Declaration><![CDATA[METHOD InitialiseStations : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[FOR i := 1 TO Xts_Library.SystemParameters.NumberOfStations DO
	iStationArray[i] := StationArray[i];
END_FOR


StationArray[1].Position := 1000.0;
StationArray[1].TempPropTime := T#1S;

StationArray[2].Position := 1200.0;
StationArray[2].TempPropTime := T#2S;

StationArray[3].Position := 1400.0;
StationArray[3].TempPropTime := T#2S;

StationArray[4].Position := 2200.0;
StationArray[4].TempPropTime := T#1S;

StationArray[5].Position := 2600.0;
StationArray[5].TempPropTime := T#1S;

StationArray[6].Position := 3200.0;
StationArray[6].TempPropTime := T#1S;



InitialiseStations := true;]]></ST>
      </Implementation>
    </Method>
