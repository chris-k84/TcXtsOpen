  <POU Name="XtsMoverAxisTestSuite" Id="{4492a4d6-579d-4fbd-b1c3-0294f878c464}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM XtsMoverAxisTestSuite
VAR
	fbTestSuites : FB_Testsuites;
	fbTestSuite : FB_Testsuite;
	
	XtsMoverAxisObj : XtsMoverAxis;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[fbTestSuites();
fbTestSuites.M_Add(fbTestSuite);
fbTestSuite();

CASE fbTestSuite.P_Testcase OF
	E_Testcase.Start:
		fbTestSuite.P_Assert.M_Equals_INT(1, XtsMoverAxisObj.Cycle(), "Cycle method did not return a complete");
		fbTestSuite.M_Testcase('MoverCycleShouldReturnACompleteSignal',T#1S,'','',1);
			 
	1:
		fbTestSuite.P_Assert.M_Equals_INT(1, XtsMoverAxisObj.EnableMover(), "Enable command failed when mover disabled ");
		fbTestSuite.M_Testcase('MoverClassShouldAcceptEnableCommandWhenDisabled',T#1S,'','',2);
		
	2:
		fbTestSuite.P_Assert.M_Equals_INT(-1, XtsMoverAxisObj.Cycle(), "Enable Command was accepted when enabled ");
		fbTestSuite.M_Testcase('MoverClassShouldRejectEnableCommandWhenEnabled',T#1S,'','',E_Testcase.Done);
		
	E_Testcase.Done:;	 
END_CASE

]]></ST>
    </Implementation>
