    <Method Name="Abort" Id="{5c5502fe-cfbb-4721-939d-6b8391fd3812}">
      <Declaration><![CDATA[METHOD Abort : BOOL
]]></Declaration>
      <Implementation>
        <ST><![CDATA[moveAbs.Execute := FALSE;
moveAbs.Position := moveAbs.Velocity := 0;
_MoveMover := false;
_Busy := FALSE;
Abort := true;
State := 0;]]></ST>
      </Implementation>
    </Method>
