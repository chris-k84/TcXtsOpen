    <Method Name="ActivateTrack" Id="{f7604675-5338-4e11-9132-b17d5df72cc8}" FolderPath="Commands\">
      <Declaration><![CDATA[METHOD ActivateTrack : HRESULT
VAR_INPUT
	NewTrack : OTCID;
	Track : I_XtsTrack;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT MoverActivateTrackCommand.Busy AND THIS^.isMoverStationary() AND NewTrack <> 0  THEN
	IF MoverActivateTrackCommand.Execute(Track := Track.MyOTCID) THEN
		_CurrentTrack := Track;
		_ActiveTrackLength := Track.TrackLength;
		ActivateTrack := 1;
	ELSE
		ActivateTrack := -1;
	END_IF
ELSE
	ActivateTrack := -1;
END_IF]]></ST>
      </Implementation>
    </Method>
