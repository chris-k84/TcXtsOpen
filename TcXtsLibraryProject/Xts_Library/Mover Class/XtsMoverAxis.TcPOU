    <Method Name="Gear" Id="{b0aa32af-e1ad-43df-b90e-7e7d830bc963}" FolderPath="Commands\">
      <Declaration><![CDATA[METHOD PUBLIC Gear : HRESULT
VAR_INPUT
	MasterPosition : MC_LREAL;
	SlavePosition : MC_LREAL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT axesGear.Busy AND __ISVALIDREF(axesGear.MyGearMaster) AND MasterPosition <= _ActiveTrackLength AND SlavePosition <= _ActiveTrackLength THEN
	IF axesGear.Sync(MasterPosition := MasterPosition, SlavePosition := SlavePosition) THEN
		Gear := 1;
	ELSE
		Gear := -1;
	END_IF
ELSE
	Gear := -1;
END_IF
]]></ST>
      </Implementation>
    </Method>
